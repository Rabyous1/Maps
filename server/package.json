{
    "name": "pentabell-maps-backend",
    "version": "1.0.0",
    "description": "pentabell-maps-backend",
    "main": "./dist/server.js",
    "scripts": {
        "start": "tsc && cross-env NODE_ENV=dev nodemon dist/server.js",
        "start:test": "tsc && cross-env NODE_ENV=test node dist/server.js",
        "start:prod": "tsc && cross-env NODE_ENV=prod node dist/server.js",
        "dev": "cross-env NODE_ENV=dev tsc-watch --onSuccess \"node ./dist/server.js\"",
        "build": "tsc && cpx \"./src/utils/services/email-service/**/\" ./dist/utils/services/email-service/ --clean && cpx \"./src/public/**/\" ./dist/public/ --clean && cpx \"./src/utils/data/**/\" ./dist/utils/data/ --clean  && cpx \"./uploads/**/\" ./dist/uploads/ --clean",
        "lint": "eslint . --ext .ts --fix",
        "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles --forceExit",
        "test:watch": "cross-env NODE_ENV=test jest --watch",
        "test:coverage": "cross-env NODE_ENV=test jest --coverage --runInBand --detectOpenHandles --forceExit",
        "test:unit": "cross-env NODE_ENV=test jest --testPathIgnorePatterns=integration --runInBand --detectOpenHandles --forceExit",
        "test:integration": "cross-env NODE_ENV=test jest src/tests/integration/ --runInBand --detectOpenHandles --forceExit",
        "test:integration:watch": "cross-env NODE_ENV=test jest src/tests/integration/simple.integration.test.ts src/tests/integration/controllers.integration.test.ts --watch",
        "test:all": "npm run test:unit && npm run test:integration"
    },
    "keywords": [
        "pentabell",
        "cv",
        "jobs"
    ],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "^2.8.13",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.17",
        "@types/express-session": "^1.18.1",
        "@types/geoip-lite": "^1.4.4",
        "@types/jest": "^30.0.0",
        "@types/node": "^18.15.11",
        "@types/node-cron": "^3.0.11",
        "@types/passport": "^1.0.17",
        "@types/passport-azure-ad": "^4.3.6",
        "@types/passport-google-oauth20": "^2.0.16",
        "@types/passport-linkedin-oauth2": "^1.5.6",
        "@types/passport-microsoft": "^1.0.3",
        "@types/qrcode": "^1.5.5",
        "@types/socket.io": "^3.0.1",
        "@types/speakeasy": "^2.0.10",
        "@types/supertest": "^2.0.16",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/uuid": "^10.0.0",
        "@types/yamljs": "^0.2.34",
        "@typescript-eslint/eslint-plugin": "^5.58.0",
        "@typescript-eslint/parser": "^5.58.0",
        "class-transformer": "^0.5.1",
        "cpx": "^1.2.1",
        "cross-env": "^7.0.3",
        "eslint": "^8.38.0",
        "jest": "^30.1.3",
        "supertest": "^7.1.4",
        "ts-jest": "^29.4.1",
        "ts-node": "^10.9.2",
        "tsc-watch": "^6.0.0",
        "typescript": "^5.8.3"
    },
    "dependencies": {
        "@handlebars/allow-prototype-access": "^1.0.5",
        "@jest/types": "^29.6.3",
        "@radix-ui/react-tooltip": "^1.2.7",
        "@types/bcrypt": "^5.0.0",
        "@types/body-parser": "^1.19.2",
