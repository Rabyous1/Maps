        "csv-parser": "^3.2.0",
        "date-fns": "^3.3.1",
        "dotenv": "^16.4.7",
        "envalid": "^7.3.1",
        "express": "^4.18.2",
        "express-handlebars": "^7.1.2",
        "express-session": "^1.18.1",
        "fs": "^0.0.1-security",
